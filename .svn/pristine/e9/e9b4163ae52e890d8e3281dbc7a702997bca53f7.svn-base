#mainContainer {
            height: 100%;
            position: relative;
        }
        
        body {
            width: 100%;
            height: 100%;
            text-align: center;
            font-family: 'Raleway', sans-serif;
            line-height: 0.42857143
        }
        
        #innerContainer {
            height: 100%;
            overflow-y: scroll;
            font-size: 48px;
            color: #fff;
            position: absolute;
            width: 100%;
            background-color: rgba(0,0,0,0.6);
            line-height: 100%
        }
        
        .text {
            text-align: center;
        }
        
        @-webkit-keyframes ani-mouse {
            0% {
                opacity: 1;
                top: 29%;
            }
            15% {
                opacity: 1;
                top: 50%;
            }
            50% {
                opacity: 0;
                top: 50%;
            }
            100% {
                opacity: 0;
                top: 29%;
            }
        }
        
        @-moz-keyframes ani-mouse {
            0% {
                opacity: 1;
                top: 29%;
            }
            15% {
                opacity: 1;
                top: 50%;
            }
            50% {
                opacity: 0;
                top: 50%;
            }
            100% {
                opacity: 0;
                top: 29%;
            }
        }
        
        @keyframes ani-mouse {
            0% {
                opacity: 1;
                top: 29%;
            }
            15% {
                opacity: 1;
                top: 50%;
            }
            50% {
                opacity: 0;
                top: 50%;
            }
            100% {
                opacity: 0;
                top: 29%;
            }
        }
        
        .scroll-btn {
            position: absolute;
            width: 150px;
            bottom: 20px;
            left: 50%;
            margin-left: -75px;
            z-index: 50;
            display: block;
            text-align: center;
            font-size: 12px;
        }
        
        .scroll-btn > * {
            display: inline-block;
            line-height: 18px;
            font-size: 13px;
            font-weight: normal;
            color: #7f8c8d;
            color: #ffffff;
            letter-spacing: 2px;
            opacity: 0.8;
        }
        
        .scroll-btn > *:hover,
        .scroll-btn > *:focus,
        .scroll-btn > *.active {
            color: #ffffff;
        }
        
        .scroll-btn > *:hover,
        .scroll-btn > *:focus,
        .scroll-btn > *:active,
        .scroll-btn > *.active {
            opacity: 0.8;
            filter: alpha(opacity=80);
        }
        
        .scroll-btn .mouse {
            position: relative;
            display: block;
            width: 31px;
            height: 51px;
            margin: 0 auto 2px;
            -webkit-box-sizing: border-box;
            -moz-box-sizing: border-box;
            box-sizing: border-box;
            border: 3px solid white;
            border-radius: 23px;
        }
        
        .scroll-btn .mouse > * {
            position: absolute;
            display: block;
            top: 29%;
            left: 50%;
            width: 8px;
            height: 8px;
            margin: -4px 0 0 -4px;
            background: white;
            border-radius: 50%;
            -webkit-animation: ani-mouse 2.5s linear infinite;
            -moz-animation: ani-mouse 2.5s linear infinite;
            animation: ani-mouse 2.5s linear infinite;
        }
        
        video {
            object-fit: fill;
        }
        
        header {
            position: fixed;
            z-index: 9999;
            top: 0;
            left: 0;
            width: 100%;
        }
        .active .progress-border {
            height: 5px;
            background-color: rgba(255, 255,255, 0.6);
            margin-top: 8px;
        }
        
        .progress-border .progress-line{
            height: 100%;
            background-color: rgba(255, 255,255, 1);
            width : 0%;
        }
		
