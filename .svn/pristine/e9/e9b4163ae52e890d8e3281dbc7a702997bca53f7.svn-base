.splitter {
    height: 100%;
    position: relative;
    width: 100%;
	background: url("../images/Group.png") no-repeat scroll 0 0 / cover rgba(0, 0, 0, 0);
}
.splitter .splitter-item:first-child {
    left: 0;
}
.splitter .splitter-item {
    text-align: center;
    transition: background-color 0.4s cubic-bezier(0.7, 0, 0.3, 1) 0s;
}
.splitter .splitter-item {
    height: 100%;
    float: left;
    top: 0;
   /* width: 50%; */
}
.vertical-container {
    display: table;
    height: 100%;
}
.main-container, .vertical-container, .video-container {
    position: relative;
    width: 100%;
}
.vertical-container .vertical-content {
    display: table-cell;
    position: relative;
    vertical-align: middle;
}
.splitter .splitter-button {
    color: #320d7f;
    display: inline-block;
    font-size: 32px;
    height: 120px;
    line-height: 1.1;
    padding: 18px;
    position: relative;
    text-transform: uppercase;
    transition: color 0.4s cubic-bezier(0.7, 0, 0.3, 1) 0s;
    width: 280px;
    z-index: 1;
}
.splitter .splitter-item:hover  {
    background-color: rgba(0, 0, 0, 0.55);
	
}
.splitter .splitter-button:hover:before {
    background-color: grey;
}
.splitter .splitter-button:before {
    /*border-color: #320d7f;
    border-style: solid;
    border-width: 12px; */
	border : 12px solid grey;
    content: "";
    display: block;
    height: 120px;
    left: 0;
    position: absolute;
    top: -6px;
    transition: all 0.4s cubic-bezier(0.7, 0, 0.3, 1) 0s;
    width: 280px;
    z-index: -1;
}
.splitter .splitter-item.active .splitter-button, .splitter .splitter-button:hover {
    color: #320d7f !important;
}
.splitter .splitter-item:hover   a {    
	
	text-decoration : none;
}
.splitter .splitter-item.active .splitter-button:before, .splitter .splitter-item:hover .splitter-button:before {
    border-color: #fff;
    border-radius: 50%;
    height: 280px;
    top: -89px;
    width: 280px;
	
	background-color : #fff;
}


